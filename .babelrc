{
    "presets": [
        "react",
        [ "env", {
            "targets": {
                "browsers": "Last 2 versions"
            },
            "loose": true,
            "modules": false
        }]
    ],
    "plugins": [
        "babel-plugin-transform-class-properties"
    ],
    "env": {
        "test":{
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}